   "source": [
    "# Finding Basic Textual Patterns in Strings\n",
    "\n",
    "\n",
    "## Dependencies\n",
    "import re\n",
    "\n",
    "## Data\n",
    "text = 'peter piper picked a peck of pickled peppers'\n",
    "\n",
    "## One-Liner\n",
    "result = re.findall('p.*?e.*?r', text)\n",
    "\n",
    "## Result\n",
    "print(result)\n",
    "'''\n",
    "['peter', 'piper', 'picked a peck of pickled pepper']\n",
    "'''\n"
   ]
  }
